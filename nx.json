{
  "npmScope": "platon",
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tslint.json": "*",
    "nx.json": "*",
    "tsconfig.base.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "platon": {
      "tags": ["scope:platon"]
    },
    "platon-e2e": {
      "tags": [],
      "implicitDependencies": ["platon"]
    },
    "doc": {
      "tags": ["scope:doc"]
    },
    "doc-e2e": {
      "tags": [],
      "implicitDependencies": ["doc"]
    },
    "shared-ui": {
      "tags": ["scope:shared", "type:ui"]
    },
    "shared-utils": {
      "tags": ["scope:shared", "type:utils"]
    },
    "shared-vendors": {
      "tags": ["scope:shared", "type:vendors"]
    },
    "features-web-components": {
      "tags": ["scope:feature", "type:web-components"]
    }
  },
  "affected": {
    "defaultBase": "master"
  }
}
