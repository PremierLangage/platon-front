{
  "name": "platon-front",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "node ./decorate-angular-cli.js && ngcc",
    "ng": "nx",
    "nx": "nx",
    "start": "ng serve",
    "build": "npm run build:doc && npm run build:platon",
    "build:doc": "nx build doc --prod --extra-webpack-config webpack.externals.js --single-build --stats-json --base-href='https://premierlangage.github.io/platon-front/'",
    "build:platon": "nx build platon --prod --extra-webpack-config webpack.externals.js --single-build --stats-json",
    "publish:doc": "angular-cli-ghpages --dir=./dist/apps/doc",
    "stats:doc": "webpack-bundle-analyzer dist/apps/doc/stats-es2015.json",
    "stats:platon": "webpack-bundle-analyzer dist/apps/platon/stats-es2015.json",
    "test": "ng test --code-coverage --watch=false",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.0",
    "@angular/cdk": "^9.1.0",
    "@angular/common": "^9.1.0",
    "@angular/compiler": "^9.1.0",
    "@angular/core": "^9.1.0",
    "@angular/elements": "^9.1.11",
    "@angular/forms": "^9.1.0",
    "@angular/material": "^9.1.0",
    "@angular/platform-browser": "^9.1.0",
    "@angular/platform-browser-dynamic": "^9.1.0",
    "@angular/router": "^9.1.0",
    "@juristr/ngx-lazy-el": "^1.0.1",
    "@nrwl/angular": "^9.4.5",
    "@webcomponents/webcomponentsjs": "^2.4.3",
    "ang-jsoneditor": "^1.10.2",
    "animate.css": "^4.1.1",
    "common-tags": "^1.8.0",
    "document-register-element": "^1.7.2",
    "hammerjs": "^2.0.8",
    "jsoneditor": "^9.1.1",
    "jsplumb": "^2.14.6",
    "jsxgraph": "^1.1.0",
    "marked": "^1.1.1",
    "mathlive": "^0.55.0",
    "monaco-editor": "^0.21.2",
    "nge-doc": "^1.0.0",
    "nge-markdown": "^1.0.0",
    "nge-monaco": "^1.0.1",
    "ngx-build-plus": "^9.0.6",
    "rxjs": "~6.5.4",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "^9.1.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nrwl/workspace": "9.4.5",
    "@types/common-tags": "^1.8.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/json-schema": "^7.0.6",
    "@types/marked": "^1.1.0",
    "@types/node": "^8.9.5",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "~5.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "6.2.0",
    "eslint": "6.8.0",
    "husky": "^4.3.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "prettier": "2.0.4",
    "protractor": "^7.0.0",
    "standard-version": "^9.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.0.0",
    "typescript": "~3.8.3",
    "webpack-bundle-analyzer": "^3.8.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
