{
  "version": 1,
  "projects": {
    "platon": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/platon",
      "sourceRoot": "apps/platon/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "outputPath": "dist/apps/platon",
            "index": "apps/platon/src/index.html",
            "main": "apps/platon/src/main.ts",
            "polyfills": "apps/platon/src/polyfills.ts",
            "tsConfig": "apps/platon/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/platon/src/favicon.ico",
              "apps/platon/src/assets",
              {
                "glob": "**/*",
                "input": "libs/shared/assets/src",
                "output": "./assets"
              }
            ],
            "styles": ["apps/platon/src/styles.scss"],
            "scripts": [
              {
                "bundleName": "polyfill-webcomp-es5",
                "input": "node_modules/@webcomponents/webcomponentsjs/custom-elements-es5-adapter.js"
              },
              {
                "bundleName": "polyfill-webcomp",
                "input": "node_modules/@webcomponents/webcomponentsjs/bundles/webcomponents-sd-ce-pf.js"
              },
              "node_modules/rxjs/bundles/rxjs.umd.js",
              "node_modules/@angular/core/bundles/core.umd.js",
              "node_modules/@angular/common/bundles/common.umd.js",
              "node_modules/@angular/common/bundles/common-http.umd.js",
              "node_modules/@angular/compiler/bundles/compiler.umd.js",
              "node_modules/@angular/elements/bundles/elements.umd.js",
              "node_modules/@angular/platform-browser/bundles/platform-browser.umd.js",
              "node_modules/@angular/platform-browser-dynamic/bundles/platform-browser-dynamic.umd.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/platon/src/environments/environment.ts",
                  "with": "apps/platon/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "options": {
            "browserTarget": "platon:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "platon:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "platon:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/platon/tsconfig.app.json",
              "apps/platon/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/platon/**/*"]
          }
        },
        "test": {
          "builder": "ngx-build-plus:karma",
          "options": {
            "main": "apps/platon/src/test.ts",
            "tsConfig": "apps/platon/tsconfig.spec.json",
            "karmaConfig": "apps/platon/karma.conf.js",
            "polyfills": "apps/platon/src/polyfills.ts",
            "styles": [],
            "scripts": [],
            "assets": []
          }
        }
      }
    },
    "platon-e2e": {
      "root": "apps/platon-e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/platon-e2e/protractor.conf.js",
            "devServerTarget": "platon:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "platon:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/platon-e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**", "!apps/platon-e2e/**/*"]
          }
        }
      }
    },
    "shared-styles": {
      "projectType": "library",
      "root": "libs/shared/styles",
      "sourceRoot": "libs/shared/styles/src",
      "prefix": "styles",
      "architect": {},
      "schematics": {}
    },
    "shared-assets": {
      "projectType": "library",
      "root": "libs/shared/assets",
      "sourceRoot": "libs/shared/assets/src",
      "prefix": "assets",
      "architect": {},
      "schematics": {}
    },
    "shared-ui": {
      "projectType": "library",
      "root": "libs/shared/ui",
      "sourceRoot": "libs/shared/ui/src",
      "prefix": "ui",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/shared/ui/tsconfig.lib.json",
              "libs/shared/ui/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/shared/ui/**/*"]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/shared/ui/src/test.ts",
            "tsConfig": "libs/shared/ui/tsconfig.spec.json",
            "karmaConfig": "libs/shared/ui/karma.conf.js"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    },
    "shared-utils": {
      "projectType": "library",
      "root": "libs/shared/utils",
      "sourceRoot": "libs/shared/utils/src",
      "prefix": "utils",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/shared/utils/tsconfig.lib.json",
              "libs/shared/utils/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/shared/utils/**/*"]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/shared/utils/src/test.ts",
            "tsConfig": "libs/shared/utils/tsconfig.spec.json",
            "karmaConfig": "libs/shared/utils/karma.conf.js"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    },
    "shared-vendors": {
      "projectType": "library",
      "root": "libs/shared/vendors",
      "sourceRoot": "libs/shared/vendors/src",
      "prefix": "vendors",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/shared/vendors/tsconfig.lib.json",
              "libs/shared/vendors/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/shared/vendors/**/*"]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/shared/vendors/src/test.ts",
            "tsConfig": "libs/shared/vendors/tsconfig.spec.json",
            "karmaConfig": "libs/shared/vendors/karma.conf.js"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    },
    "webcomponents": {
      "projectType": "library",
      "root": "libs/webcomponents",
      "sourceRoot": "libs/webcomponents/src",
      "prefix": "wc",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/webcomponents/tsconfig.lib.json",
              "libs/webcomponents/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/webcomponents/**/*"]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/webcomponents/src/test.ts",
            "tsConfig": "libs/webcomponents/tsconfig.spec.json",
            "karmaConfig": "libs/webcomponents/karma.conf.js"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "karma",
      "e2eTestRunner": "protractor"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "karma"
    }
  },
  "defaultProject": "platon"
}
